## Customize these properties to develop with local dependencies

##
# Here values is the same as ant basedir because basedir is not set in this context
#
# Please set this value first and with an absolute path.
##
#work.dir=/mnt/c/data/git-repos/xrechnung-testsuite


#xrechnung.version.full=2.0.0

##
# Set this property in order to activate targets for local development
# Be aware the presence of this property determines some of the behavior independent of the value
##
# develop.local.validator=true



###
# Point to local copy of Validator Configuration XRechnung project directory
# Example is relative to this project directory in local file system
##
#validator.configuration.download.url.prefix=file://${work.dir}/../validator-configuration-xrechnung/

# this must be a direct absolute path 
#validator.configuration.project.dir=/mnt/c/data/git-repos/validator-configuration-xrechnung
#validator.configuration.resources.dir=${validator.configuration.project.dir}/build/resources


#validator.version.full=1.3.1-SNAPSHOT
##
# Should point to main directory of validator project, in case of local path
##
#validator.dir=${validator.configuration.project.dir}/lib

#validator.jar=validationtool-${validator.version.full}-java8-standalone.jar

